@{Layout = null;}
@model Competition_Blasts_Project.Models.Player
@{
    string fname = Session["FName"].ToString();
    string lname = Session["LName"].ToString();
    string cover = Session["Cover"].ToString();
    string profile = Session["Profile"].ToString();
    string score1 = Session["P1Score"].ToString();
    string score2 = Session["P2Score"].ToString();
    int p1Score = int.Parse(score1);
    int p2score = int.Parse(score2);
    string sub = Session["Subject"].ToString();

}
<html>
<head>
    <title> Result </title>
    <link href="~/Content/Style.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.2.3.js"></script>
    <script>
        function announce()
        {
            var p1 = '@p1Score';
            var p2 = '@p2score';
            if(p1>p2)
                $("#winner").html('@fname @lname');
            else
                $("#winner").html('@Model.FirstName @Model.LastName');
        }
    </script>
</head>
<body class="loadBody" onload="announce()">
    <center>
        <h2 style="color:#888;"> RESULTS </h2>
        <h2 style="color:#eeeeee; "> @sub</h2>
    </center>
    <table border="0" style="margin-top : 2.5%;">
        <tr style="height : 75%;">
            <td style="width : 25%;">
                <h3 class="resultScore" style="margin-left: 50%;">@p1Score</h3>
                <h3 class="resultPlayerName" style="margin-left: 20%;">@fname @lname</h3>
                <img src="@Url.Content(profile)" id="resultProfilePic" style="margin-left: 65%;">
            </td>
            <td style="width : 25%;">
                <h3 class="resultScore" style="margin-left: 44%;">@p2score</h3>
                <h3 class="resultPlayerName" style="margin-left: 45%;">@Model.FirstName @Model.LastName</h3>
                <img src="@Url.Content(Model.ProfileImagePath)" id="resultProfilePic" style="margin-left: 15%;">
            </td>
        </tr>
    </table>

    <br><br><br><br>
    <table border="1" class="scoreChart">
        <tr>
            <td style="font-size:20px; color:#ff9933">Correct Answers</td>
            <td style="font-size:20px; color:#66cc66">Wrong Answers</td>
            <td style="font-size:20px; color:#0099ff">Bonus Points</td>
        </tr>
        <tr></tr>
        <tr></tr>
        <tr></tr>
        <tr>
            <td style="text-align: center; border-color:#ff9933; font-size:20px; color:#ff9933">@p1Score</td>
            <td style="text-align: center; border-color:#66cc66; font-size:20px; color:#66cc66">0</td>
            <td style="text-align: center; border-color:#0099ff; font-size:20px; color:#0099ff">0</td>
        </tr>
    </table>

    <table border="1" style="border-style:hidden; border-color:#333; float:right; margin-right:10%;">
        <tr>
            <td style="font-size:20px; color:#ff9933">Correct Answers</td>
            <td style="font-size:20px; color:#66cc66">Wrong Answers</td>
            <td style="font-size:20px; color:#0099ff">Bonus Points</td>
        </tr>
        <tr></tr>
        <tr></tr>
        <tr></tr>
        <tr>
            <td style="text-align: center; border-color:#ff9933; font-size:20px; color:#ff9933">@p2score</td>
            <td style="text-align: center; border-color:#66cc66; font-size:20px; color:#66cc66">0</td>
            <td style="text-align: center; border-color:#0099ff; font-size:20px; color:#0099ff">0</td>
        </tr>
    </table>
    <div id="loginFields">
        <center>
            <h2 style="color:#888;"> WINNER</h2>
            <h2 id="winner" style="color:#eeeeee; "></h2>
        </center>
        <form name="ProfileForm" action="~/Home/Profile" method="post">
            <center>
                <input type="submit" name="submit" id="send" value="Back To Home" style="width: 100%; margin-left: 3%; margin-top: 30%;" />
            </center>
        </form>
    </div>
</body>
</html>